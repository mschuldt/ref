    <span class="bold">News: </span><span id="newsheadline"></span> <a class="arrowBlue" id="newslink" href=""></a>
</div>
<script type="text/javascript" defer="defer">

//Define the global namespaces
var Q = Q || {};
Q.NextQ = Q.NextQ || {};
Q.NextQ.Views = Q.NextQ.Views || {};
Q.NextQ.Views.NewslineView = function () {
    //Enforce that every time a constructor function is called, this function is invoked properly using the new operator
    if (false === (this instanceof Q.NextQ.Views.NewslineView)) {
        return new Q.NextQ.Views.NewslineView();
    }

    var _public = this;
    var _private = _public._private = {};

    var NEWS_SEPARATOR = String.fromCharCode(9829); //♥

    var newsitem = document.getElementById('md_c_main_content_0_newsItem').value;
    var myArray = newsitem.split(NEWS_SEPARATOR);
    var timedelay = document.getElementById('timedelay').value;
    var icount = parseInt(timedelay)
    var counter = 0;
    var millisecondsToWait = icount * 1000;
    //var millisecondsToWait = 1000;
    var headline = $('#newsheadline');
    var newslink = $('#newslink');
    var firstNews = true;
    var allLetters = [];
    var allLettersOpacities = [];
    var newsLoopHandler = null;
    var fadeIncrease = 0.05;
    var fadeInDelay = 5; //50ms
    var fadeOutDelay = 5; //50ms
    var fadeInIdx = 0;
    var arrowIndex = 0;
    var TOTAL_FADEIN_TIME = 1800; // 2000ms
    var TOTAL_FADEOUT_TIME = 300; // 1000ms

    ////////////////////////////////////////////////////////////////////////////////////////
    /* public functions*/

    _public.initializeOnReady = function () {
        var newsElement = document.getElementById('md_c_main_content_0_newsItem');
        if (newsElement == null)
            return;
        var newsitem = newsElement.value;
        //\u2665  = String.fromCharCode(9829) = ♥
        if (!newsitem || newsitem.replace(/\u2665/g, "").trim().length < 3)
            return;
        if (j$.browser.msie && j$.browser.version < "8") {
            var myArray = newsitem.split(NEWS_SEPARATOR);
            $('#newsheadline').html(myArray[0]);
            $('#newslink').html(myArray[1]);
            $('#newslink').attr('href', myArray[2]);
            setInterval(function () {
                $('#newsheadline').html(myArray[counter * 3]);
                $('#newslink').html(myArray[counter * 3 + 1]);
                $('#newslink').attr('href', myArray[counter * 3 + 2]);
                counter++;
                if (counter == myArray.length / 3)
                    counter = 0;
            }, millisecondsToWait);
        } else {
            newslink.attr("style", "background:none");
            setTimeout(fadeOut, 10);
        }
    };

    ////////////////////////////////////////////////////////////////////////////////////////
    /* private functions*/

    function totalOpacity() {
        var total = 0;
        for (var idx = 0; idx < allLettersOpacities.length; idx++) {
            total += allLettersOpacities[idx];
        }
        return total;
    }

    function fadeIn() {
        fadeInIdx = 0;
        fadeInLoop();
    };

    function truncate(_value) {
        if (_value < 0) return Math.ceil(_value);
        else return Math.floor(_value);
    }

    function setOpacity(letter, opacity) {
        var style = "";

        if (j$.browser.msie && j$.browser.version < "10") {
            var ieOpacity = truncate(opacity * 100);
            if (j$.browser.msie && j$.browser.version < "8") {
                /* IE 5-7 */
                style += 'filter: alpha(opacity=' + ieOpacity + ');';
            } else {
                /* IE 8 */
                style += '-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=' + ieOpacity + ')";';
            }
        } else {
            /* Netscape */
            style += '-moz-opacity: ' + opacity + ';';
            /* Safari 1.x */
            style += '-khtml-opacity: ' + opacity + ';'
            /* Good browsers */
            style += 'opacity: ' + opacity + ';';
        }
        letter.attr("style", style);
    }

    function fadeInLoop() {
        if (totalOpacity() < allLettersOpacities.length) {
            for (var idx2 = 0; idx2 < allLettersOpacities.length && idx2 <= fadeInIdx; idx2++) {
                if (allLettersOpacities[idx2] != 1) {
                    allLettersOpacities[idx2] += fadeIncrease;
                    if (allLettersOpacities[idx2] > 1)
                        allLettersOpacities[idx2] = 1.0;
                    setOpacity(allLetters[idx2], allLettersOpacities[idx2]);
                }
            }
            fadeInIdx++;
            if (allLettersOpacities[arrowIndex] > 0.3)
                newslink.attr("style", "");
            setTimeout(fadeInLoop, fadeInDelay);
        } else {
            setTimeout(fadeOut, millisecondsToWait);
        }
    }

    function fadeOut() {
        fadeInIdx = allLettersOpacities.length - 1;

        //fadeOut, opacity from 0 to 1
        if (!firstNews) {
            fadeOutLoop();
        } else {
            firstNews = false;
            setTimeout(loopNews, 10);
        }
    };

    function fadeOutLoop() {
        if (totalOpacity() > 0) {
            for (var idx2 = allLettersOpacities.length - 1; idx2 >= 0 && idx2 >= fadeInIdx; idx2--) {
                if (allLettersOpacities[idx2] != 0) {
                    allLettersOpacities[idx2] -= fadeIncrease;
                    if (allLettersOpacities[idx2] < 0)
                        allLettersOpacities[idx2] = 0.0;
                    setOpacity(allLetters[idx2], allLettersOpacities[idx2]);
                }
            }
            fadeInIdx--;
            if (allLettersOpacities[arrowIndex] < 0.3)
                newslink.attr("style", "background:none");
            setTimeout(fadeOutLoop, fadeOutDelay);
        } else {
            setTimeout(loopNews, 10);
        }
    }

    function injectSpans(textRootSelector) {
        j$(textRootSelector).contents().filter(function () { return this.nodeType === 3; }).each(function (index, element) {
            var textNode = j$(element);
            var lastNode = textNode;
            var str = textNode.text();
            for (var idx = 0; idx < str.length; idx++) {
                lastNode = j$("<span style='opacity: 0; filter: alpha(opacity=0)'>" + str[idx] + "</span>").insertAfter(lastNode);
            }
            textNode.remove();
        });
    }

    function loopNews() {
        //hidden
        headline.css("display", "hidden");
        newslink.css("display", "hidden");

        //remove old content
        headline.contents().remove();
        newslink.contents().remove();

        var headlineStr = "";
        var readMoreStr = "";
        var headlineLink = "";

        do {
            headlineStr = myArray[counter * 3];
            readMoreStr = myArray[counter * 3 + 1];
            headlineLink = myArray[counter * 3 + 2];
            //loop counter for the news
            counter++;
            if (counter == myArray.length / 3)
                counter = 0;
        } while (headlineStr == "")

        //insert new RAR content
        headline.html(headlineStr);
        newslink.html(readMoreStr);
        newslink.attr('href', headlineLink);

        injectSpans("#newsheadline, #newsheadline *");
        injectSpans("#newslink, #newslink *");

        //show, with opacity 0 (so, still not visible)
        headline.show();
        newslink.show();


        //fade-in, opacity from 0 to 1
        fadeInIdx = 0;
        var newsLinkSpans = newslink.find("span");
        var headlineSpans = headline.find("span");
        arrowIndex = headlineSpans.size() - 1;
        var temp = headlineSpans.add(newsLinkSpans);
        allLetters = [];
        allLettersOpacities = [];
        for (var idx = 0; idx < temp.length; idx++) {
            //make it a jquery object
            allLetters.push(j$(temp[idx]));
            //save its opacity
            allLettersOpacities.push(0.0);
        }
        calculateStep();
        setTimeout(fadeIn, 10);
    }

    function calculateStep() {
        var len = allLettersOpacities.length <= 0 ? 1 : allLettersOpacities.length;
        //var fadeIncrease = 0.025;
        var step = (len + (1 / fadeIncrease));
        fadeInDelay = TOTAL_FADEIN_TIME / step; //10ms
        fadeOutDelay = TOTAL_FADEOUT_TIME / step; //10ms
    }

    return _public;
}

if (typeof (Sys.WebForms) !== 'undefined') {
    var prm = Sys.WebForms.PageRequestManager.getInstance();

    var newslineView = new Q.NextQ.Views.NewslineView();
    prm.add_pageLoaded(newslineView.initializeOnReady);
}
</script>




